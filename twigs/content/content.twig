<article id="post-{{ wp.the_ID }}" {{ wp.post_class }}>
    <header class="entry-header">
        <h1 class="entry-title"><a href="{{ wp.the_permalink }}" title="{{ wp.the_title }}">{{ wp.the_title }}</a></h1>

        <div class="entry-meta">

        </div><!-- .entry-meta -->
    </header><!-- .entry-header -->

    <div class="entry-content">
        {{ wp.the_content }}
        {{ wp.wp_link_pages }}
    </div><!-- .entry-content -->

    <footer class="entry-footer">

    </footer><!-- .entry-footer -->
</article><!-- #post-## -->